{
  "name": "pwa-qr-code-generator",
  "private": true,
  "version": "0.0.2",
  "main": "./dist/pwa-qr-code-generator.cjs",
  "module": "./dist/pwa-qr-code-generator.mjs",
  "exports": {
    ".": {
      "require": "./dist/pwa-qr-code-generator.cjs",
      "import": "./dist/pwa-qr-code-generator.mjs"
    }
  },
  "types": "./dist/pwa-qr-code-generator.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf dist && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts --fix",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.7",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.9",
    "@types/underscore": "^1.11.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-v8": "^0.34.1",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "p-elements-core": "1.2.1",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.3",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.1",
    "qr-code-styling": "^1.6.0-rc.1",
    "rimraf": "^5.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-external": "^1.2.8",
    "vite-plugin-inject-externals": "^1.1.1",
    "vitest": "^0.34.1"
  },
  "browserslist": [
    "> 2% in NL",
    "last 2 versions"
  ]
}
